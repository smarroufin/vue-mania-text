{
  "name": "vue-mania-text",
  "description": "",
  "version": "0.0.4",
  "author": "Sylvain Marroufin <marroufin.sylvain@gmail.com>",
  "license": "MIT",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.umd.cjs"
    }
  },
  "scripts": {
    "build": "vite build && vue-tsc --emitDeclarationOnly",
    "docs:dev": "nuxt dev docs",
    "docs:build": "nuxt build docs",
    "docs:generate": "nuxt generate docs",
    "docs:preview": "nuxt preview docs",
    "docs:start": "node docs/.output/server/index.mjs",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "vue-tsc --noEmit && nuxt typecheck docs",
    "postinstall": "nuxt prepare docs",
    "release": "release-it"
  },
  "dependencies": {
    "vue": "^3.5.11"
  },
  "devDependencies": {
    "@release-it/conventional-changelog": "^8.0.2",
    "@types/node": "^20.16.10",
    "@vitejs/plugin-vue": "^5.1.4",
    "eslint": "^9.12.0",
    "nuxt": "3.13.2",
    "release-it": "^17.7.0",
    "typescript": "^5.6.2",
    "vite": "^5.4.8",
    "vue-tsc": "^2.1.6"
  }
}
